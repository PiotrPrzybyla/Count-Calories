<Window x:Class="Count_Calories.AddFromApi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Count_Calories"
        mc:Ignorable="d"
        Title="AddFromApi" Height="170" Width="350">
    <Grid>
        <DockPanel Margin="10,25,10,10">
            <StackPanel Orientation="Vertical"  >
                <Label Content="Name"  Target="{Binding ElementName=NameTextBox}"/>
                <TextBox x:Name="NameTextBox"  Text="{Binding Name}" TextChanged="NameBox_TextChanged"/>
                <Button Content="Add from list" Click="tryToFind" Width="100" Height="40"/>
            </StackPanel>
        </DockPanel>
        
    </Grid>
</Window>
